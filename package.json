{
  "name": "@zammit/eslint-config-az",
  "version": "3.0.0",
  "main": "eslint.js",
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "airbnb"
  ],
  "author": {
    "name": "Andrew Zammit",
    "email": "zammit.andrew@gmail.com"
  },
  "engines": {
    "node": ">=16.0.0",
    "yarn": ">=1.22.15"
  },
  "repository": "github.com:zamnuts/eslint-config-az",
  "license": "BSD-3-Clause",
  "scripts": {
    "format": "prettier --write '**/*.js' '**/*.json' '**/*.yml'",
    "lint": "eslint --ignore-path .gitignore --ignore-pattern='!.*' .",
    "lint:fix": "eslint --fix --ignore-path .gitignore --ignore-pattern='!.*' .",
    "prepublishOnly": "yarn test",
    "test": "yarn lint"
  },
  "dependencies": {
    "@babel/core": "^7.17.0",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-syntax-top-level-await": "^7.14.5",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0"
  },
  "peerDependencies": {
    "eslint": "^8.8.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.7.0",
    "eslint-plugin-unicorn": "^40.1.0",
    "eslint-plugin-yml": "^0.13.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "eslint": "^8.8.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.7.0",
    "eslint-plugin-unicorn": "^40.1.0",
    "eslint-plugin-yml": "^0.13.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.1"
  }
}
